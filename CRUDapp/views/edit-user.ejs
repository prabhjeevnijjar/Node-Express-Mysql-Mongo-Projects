<%- include('../views/partials/header'); -%>
<form action ="/edit/:id" method="POST">
    <div class="form-group">
        <label for="exampleInputname">Name</label>
        <input type="text" class="form-control" name = "name" id="exampleInputname" placeholder="name">
      </div>
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <input type="email" class="form-control"name="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    </div>
    <div class="form-group">
      <label for="exampleInpuInterest">Interest</label>
      <input type="text" class="form-control" name="interest" id="exampleInpuInterest" placeholder="Interest">
    </div>
    <div class="form-group">
        <label for="exampleFormControlFile1">Choose Image</label>
        <input type="file" class="form-control-file"name="image" id="exampleFormControlFile1">
      </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
  <%- include('../views/partials/footer'); -%>